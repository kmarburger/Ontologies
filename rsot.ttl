# This work was created by Anna Krusell and Kristine Marburger at the Techical University of Denmark 
# The ontology was developed in connection with the master thesis 'Graph databases for optimisation
# and design exploration with regards to indoor climate' in the autumn of 2021. 
# RSOT uses W3C's RDF technology, an open Web standard that can be freely 
# used by anyone.

@prefix rsot:    <https://example.com/terms#> .
@prefix rdfs:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix nvo:    <https://example.com/terms2#> .
@prefix owl:    <https://www.w3.org/TR/owl-ref/>.
@prefix bot:    <https://w3id.org/bot#> .
@prefix qudt:   <http://qudt.org/schema/qudt/> .
@prefix schema: <http://schema.org/> .
@prefix vs:     <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix omg:    <https://www.projekt-scope.de/ontologies/omg/#Geometry> .
@prefix cco:    <http://diarmuidr3d.github.io/cartCoord/> .
@prefix gn:     <https://www.geonames.org/ontology/ontology_v3.3.rdf> .

##########
# CLASSES
##########

#Simulation 
rsot:Measure a owl:Class ;
    rdfs:label "Measure"@en ;
    vs:term_status "stable" ;
    rdfs:isDefinedBy rsot: .

rsot:Result a owl:Class ;
    rdfs:subClassOf rsot:Measure ;
    rdfs:label "Result"@en;
    rdfs:range xsd:decimal ;
    rdfs:isDefinedBy rsot: .

rsot:SpeckleObject a owl:Class;
    rdfs:subClassOf omg:Geometry;
    rdfs:label "URL to Speckle object"@en;
    rdfs:isDefinedBy rsot: .

rsot:BoundaryCondition a owl:Class ;
    rdfs:subClassOf rsot:Measure ;
    rdfs:label "BoundaryCondition"@en;
    rdfs:isDefinedBy rsot: .

#Coordinates and measurements

rsot:Distance a owl:Class;
    rdfs:subClassOf rsot:Measure ;
    rdfs:label "Distance"@en;
    rdfs:range xsd:decimal ;
    rdfs:isDefinedBy rsot: .

rsot:Width a owl:Class;
    rdfs:subClassOf rsot:Distance;
    rdfs:label "Width"@en;
    rdfs:range xsd:decimal ;
    rdfs:isDefinedBy rsot: .

rsot:Depth a owl:Class;
    rdfs:subClassOf rsot:Distance;    
    rdfs:label "Depth"@en;
    rdfs:range xsd:decimal ;
    rdfs:isDefinedBy rsot: .

rsot:Height a owl:CLass;
    rdfs:subClassOf rsot:Distance;    
    rdfs:label "Height"@en;
    rdfs:range xsd:decimal ;
    rdfs:isDefinedBy rsot: .

rsot:Origin a owl:Class;
    rdfs:subClassOf cco:Point ;
    rdfs:label "Point of origin (WKT)"@en;
    rdfs:isDefinedBy rsot: . 

rsot:Orientation a owl:Class;
    rdfs:subClassOf rsot:Measure;
    rdfs:label "Orientation of element"@en;
    rdfs:isDefinedBy rsot: . 

#Room geometry/construction

rsot:WFR a owl:Class;
    rdfs:subClassOf rsot:Measure ;
    rdfs:label "Window Floor Ratio"@en;
    rdfs:range xsd:decimal ;
    rdfs:isDefinedBy rsot: .

rsot:Surface a owl:Class;
    rdfs:subClassOf bot:Interface;  
    rdfs:label "An Indoor Surface"@en;
    rdfs:isDefinedBy rsot: .


########
# EDGES
########

#Room geometry/construction

rsot:simulatedRoom a owl:ObjectProperty  ;
    rdfs:subPropertyOf bot:Zone ;
    rdfs:label "Relationship between result and room geometry"@en ;
    rdfs:isDefinedBy rsot: .

rsot:hasSpeckle a owl:DatatypeProperty ;
    rdfs:subPropertyOf omg:hasGeometry ;
    rdfs:label "Speckle object URL"@en ;
    rdfs:domainIncludes
        rsot:RoomType ;
    rdfs:range xsd:string ;
    vs:term_status "stable" ;
    rdfs:isDefinedBy rsot: .

rsot:hasWFR a owl:DatatypeProperty;
    rdfs:subPropertyOf rsot:hasMeasure ;
    rdfs:label "has Window Floor Ratio (WFR)"@en ;
    rdfs:range xsd:decimal ;
    rdfs:domainIncludes
        rsot:Roomtype ;
    vs:term_status "stable" ;
    rdfs:isDefinedBy rsot: .

rsot:hasRoomType a owl:DataTypeProperty ;
    rdfs:subPropertyOf omg:hasGeometry ;
    rdfs:label "has room type"@en ;
    rdfs:range xsd:string ;
    rdfs:range rsot:RoomType;
    vs:term_status "stable" ;
    rdfs:isDefinedBy rsot: .

rsot:hasSurface a owl:ObjectProperty;
    rdfs:subPropertyOf bot:hasInterface;  
    rdfs:label "An Indoor Surface"@en;
    rdfs:isDefinedBy rsot: .

#Coordinates and measurements
rsot:hasMeasure a owl:ObjectProperty;
    rdfs:label "Measure"@en ; 
    rdfs:range rsot:Measure ;
    vs:term_status "stable";
    rdfs:isDefinedBy rsot: .

rsot:hasOrigin a owl:ObjectProperty;
    rdfs:subPropertyOf cco:hasPoint ;
    rdfs:label "Point of orgin (cartesian)"@en;
    rdfs:range xsd:string;
    vs:term_status "stable";
    rdfs:isDefinedBy rsot: .

rsot:hasDistance a owl:DatatypeProperty ;
    rdfs:subPropertyOf rsot:hasMeasure ;
    rdfs:label "has Distance"@en ;
    rdfs:range xsd:decimal ;
    qudt:ucumCode "m" ;
    rsot:standardUnit "m" ;
    vs:term_status "stable" ;
    rdfs:isDefinedBy rsot: .

rsot:hasWidth a owl:DatatypeProperty ;
    rdfs:subPropertyOf rsot:hasDistance ;
    rdfs:label "has width"@en ;
    rdfs:range xsd:decimal ;
    qudt:ucumCode "m" ;
    vs:term_status "stable" ;
    rdfs:isDefinedBy rsot: .

rsot:hasDepth a owl:DatatypeProperty ;
    rdfs:subPropertyOf rsot:hasDistance;
    rdfs:label "has depth"@en ;
    rdfs:range xsd:decimal ;
    qudt:ucumCode "m" ;
    vs:term_status "stable" ;
    rdfs:isDefinedBy rsot: .

rsot:hasHeight a owl:DatatypeProperty ; 
    rdfs:subPropertyOf rsot:hasDistance ;
    rdfs:label "has Height"@en ;
    rdfs:range xsd:decimal ;
    qudt:ucumCode "m" ;
    vs:term_status "stable" ;
    rdfs:isDefinedBy rsot: .

rsot:hasOrientation a owl:DatatypeProperty ;
    rdfs:subPropertyOf rsot:hasMeasure ;
    rdfs:label "Orientation"@en ;
    rdfs:domainIncludes
        bot:Element ;
    rdfs:range xsd:decimal ;
    qudt:ucumCode "deg" ;
    rsot:standardUnit "deg" ;
    vs:term_status "stable" ;
    rdfs:isDefinedBy rsot: .

rsot:hasWallCenterPoint a owl:DatatypeProperty ;
    rdfs:subPropertyOf cco:hasPoint ;
    rdfs:label "Center point of wall, used for placement of windows"@en ;
    rdfs:range xsd:string ;
    rdfs:domainIncludes
        bot:Element ;
    vs:term_status "stable" ;
    rdfs:isDefinedBy rsot: .

rsot:hasIfcCartesianPoint a owl:DatatypeProperty ;
    rdfs:subPropertyOf cco:hasPoint ;
    rdfs:label "2D point defined to place window in wall"@en ;
    rdfs:range xsd:point ; 
    rdfs:domainIncludes
        bot:Element ;
    vs:term_status "stable" ;
    rdfs:isDefinedBy rsot: .

rsot:hasTestSurface a owl:DataTypeProperty ;
    rdfs:subPropertyOf rsot:Measure ;
    rdfs:domainIncludes
        rsot:Result ;
    rdfs:label "The test surface, can be given by a list of points(polyline) or a surface."
    rdfs:isDefinedBy rsot: .

#Simulation 

rsot:simulatedResult a owl:DatatypeProperty  ;
    rdfs:subPropertyOf rsot:hasMeasure ;
    rdfs:domainIncludes
        rsot:Result ;
    rdfs:label "Relationship between result ID and result"@en ;
    rdfs:range xsd:decimal ;
    rdfs:isDefinedBy rsot: .

rsot:hasBoundaryCondition a owl:ObjectProperty ;
    rdfs:subPropertyOf rsot:hasMeasure ;
    rdfs:label "Boundary condition for a simulation"@en ;
    rdfs:domainIncludes
        rsot:BoundaryCondition ;
    vs:term_status "stable" ;
    rdfs:isDefinedBy rsot: .



#####################
# Labels and comments
#####################

rsot:Result rdfs:comment
        "The simulated result. A simulated value, that can be assigned any unit. Boundary conditions can be assigned using the rsot: or nvo: ontologies, or by adding others."@en .

rsot:RoomType rdfs:comment
        "etc."@en .

rsot:simulatedRoom rdfs:comment
        "This relationship between the simulated room type and the given result name/ID. The result ID should have defined relationships with all given boundary conditions."@en .

rsot:Width rdfs:comment
        "The width of the room, measured internally..  "@en .

rsot:hasWidth rdfs:comment
        "The relationship between a roomtype/geometry and the width."@en .

rsot:Depth rdfs:comment
        "The depth of the room, measured internally"@en .

rsot:hasDepth rdfs:comment
        "The relationship between a roomtype/geometry and the depth."@en .

rsot:WFR rdfs:comment
        "The window floor ratio, calculated by dividing the window area/opening area with the internal floor area."@en .

rsot:hasWFR rdfs:comment
        "The relationship between a roomtype/geometry and the window floor ratio."@en .

rsot:hasBoundaryCondition rdfs:comment 
        "the relationship between a result and any boundary condition. Many boundary conditions are described in other ontologies"@en .